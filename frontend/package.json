{
    "name": "rsud-anugerah-frontend",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev -p 3000",
        "dev:stable": "NODE_OPTIONS='--max-old-space-size=4096' NEXT_TELEMETRY_DISABLED=1 next dev -p 3000",
        "dev:fast": "NEXT_PRIVATE_SKIP_VALIDATION=1 next dev -p 3000",
        "dev:ultra": "NODE_OPTIONS='--max-old-space-size=8192' NEXT_TELEMETRY_DISABLED=1 NEXT_PRIVATE_SKIP_VALIDATION=1 next dev -p 3000",
        "dev:no-cache": "WEBPACK_DISABLE_CACHE=true DISABLE_SOURCE_MAPS=true next dev -p 3000",
        "dev:fix-cache": "./fix-webpack-cache.sh",
        "dev:fix-chunks": "./fix-chunks.sh",
        "dev:turbo": "./turbo-dev.sh",
        "dev:clean": "rm -rf .next && next dev -p 3000",
        "dev:memory": "NODE_OPTIONS='--max-old-space-size=4096' next dev -p 3000",
        "dev:network": "next dev -p 3000 -H 0.0.0.0 --turbo",
        "build": "NODE_OPTIONS='--max-old-space-size=4096' next build",
        "build:docker": "NODE_ENV=production NODE_OPTIONS='--max-old-space-size=4096' next build",
        "build:analyze": "ANALYZE=true next build",
        "start": "next start",
        "start:network": "next start -p 3000 -H 0.0.0.0",
        "lint": "next lint",
        "lint:fix": "next lint --fix",
        "type-check": "tsc --noEmit",
        "test": "jest",
        "clean": "rm -rf .next out || true",
        "predev": "npm run clean"
    },
    "dependencies": {
        "@hookform/resolvers": "^5.0.1",
        "@types/jsonwebtoken": "^9.0.10",
        "@types/react-big-calendar": "^1.16.1",
        "axios": "^1.9.0",
        "clsx": "^2.1.1",
        "critters": "^0.0.23",
        "framer-motion": "^12.16.0",
        "js-cookie": "^3.0.5",
        "jsonwebtoken": "^9.0.2",
        "lucide-react": "^0.516.0",
        "moment": "^2.30.1",
        "next": "^15.3.3",
        "react": "^18",
        "react-big-calendar": "^1.18.0",
        "react-calendar": "^5.1.0",
        "react-dom": "^18",
        "react-hook-form": "^7.56.4",
        "react-hot-toast": "^2.5.2",
        "recharts": "^2.15.3",
        "zod": "^3.25.30"
    },
    "devDependencies": {
        "@types/axios": "^0.9.36",
        "@types/jest": "^29.5.14",
        "@types/js-cookie": "^3.0.6",
        "@types/node": "^20",
        "@types/react": "^18",
        "@types/react-dom": "^18",
        "autoprefixer": "^10.4.21",
        "eslint": "^8",
        "eslint-config-next": "^15.3.3",
        "jest": "^29.7.0",
        "postcss": "^8.5.3",
        "tailwindcss": "^3.4.17",
        "ts-jest": "^29.3.4",
        "typescript": "^5"
    }
}
